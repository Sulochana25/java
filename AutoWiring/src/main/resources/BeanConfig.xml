<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="beanB" class="com.maven.spring.AutoWiring.BeanB"></bean>
	<bean id="beanC" class="com.maven.spring.AutoWiring.BeanC"></bean>
	<!-- <bean id="beanDup" class="com.maven.spring.AutoWiring.BeanC"></bean> -->
	<bean id="beanByName" class="com.maven.spring.AutoWiring.BeanA" autowire="byName"></bean>
	<bean id="beanByType" class="com.maven.spring.AutoWiring.BeanA" autowire="byType"></bean>
	<bean id="beanNoAutoWire" class="com.maven.spring.AutoWiring.BeanA" autowire="no"></bean>
	<bean id="beanConstructor" class="com.maven.spring.AutoWiring.BeanD" autowire="constructor"></bean>
	
	
	
	
</beans>